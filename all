cargo build
cd tester/tools
cargo build --bin vis
cd ../..


sz=50
sum=0

for ((i=0;i<$sz;i++))
do
echo $i
num=$(printf "%04d" $i)
./target/debug/main < tester/tools/in/$num.txt > tester/tools/out/$num.txt
cd tester/tools
./target/debug/vis in/$num.txt out/$num.txt svg/$num.svg > score/$num.txt
add=`cat score/$num.txt`
sum=$(( $sum + $add ))
cd ../..
done

echo $sum



cargo build

sz=50

for ((i=0; i<$sz; i++))
do
echo $i
num=$(printf "%04d" $i)
./target/debug/main < tester/tools/in/$num.txt > tester/tools/out/$num.txt &
done
wait

cd tester/tools
cargo build --bin vis
for ((i=0; i<$sz; i++))
do
num=$(printf "%04d" $i)
./target/debug/vis in/$num.txt out/$num.txt svg/$num.svg > score/$num.txt
done

sum=0
for ((i=0;i<$sz;i++))
do
num=$(printf "%04d" $i)
add=`cat score/$num.txt`
sum=$(( $sum + $add ))
done

cd ../..

echo $sum


